{
    "name": "savvywombat/wiki-lite",
    "description": "Wiki-Lite: light weight wiki",
    "keywords": ["wiki", "savvywombat", "lite", "light"],
    "license": "MIT",
    "type": "project",
    "require": {
        "graham-campbell/markdown": "^8.0",
        "webpatser/laravel-uuid": "^3.0",
        "webuni/commonmark-attributes-extension": "^0.5.0",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.1",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "SavvyWombat\\WikiLite\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SavvyWombat\\WikiLite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SavvyWombat\\WikiLite\\ServiceProvider"
            ]
        }
    }
}
